{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "JEMML Data Processor",
  "description": "This schema defines operations performed by examples in the JEMML library",
  "type": "object",
    "properties": {
        "type": {
            "type": "string",
            "title": "Action",
            "description": "The target machine learning action to perform",
            "enum": [
                "PREPROCESS_DATA",
                "CLASSIFY_DATA",
                "TRAIN_PREPROCESSOR",
                "TRAIN_CLASSIFIER",
                "GENERATE_ERR"
            ]
        },
        "preprocessors": {
            "type": "object",
            "properties": {
                "isFile": {
                    "type": "boolean",
                    "title": "Load From File",
                    "enum": [
                        true,
                        false
                    ]
                }
            },
            "dependencies": {
                "isFile": {
                    "oneOf": [
                        {
                            "properties": {
                                "isFile": {
                                    "enum": [
                                        true
                                    ]
                                },
                                "filePath": {
                                    "type": "string",
                                    "title": "File Path"
                                }
                            }
                        },
                        {
                            "properties": {
                                "isFile": {
                                    "enum": [
                                        false
                                    ]
                                },
                                "proprecessorList": {
                                    "type": "array",
                                    "title": "Preprocessors",
                                    "items": {
                                        "$ref": "#/definitions/preprocessor"
                                    }
                                }
                            }
                        }
                    ]
                }
            }
        }
    },
    "dependencies": {
        "type": {
            "oneOf": [
                {
                    "properties": {
                        "type": {
                            "enum": [
                                "PREPROCESS_DATA"
                            ]
                        },
                        "input": {
                            "type": "object",
                            "$ref": "#/definitions/processorInput"
                        },
                        "output": {
                            "$ref": "#/definitions/output/single"
                        }
                    }
                },
                {
                    "properties": {
                        "type": {
                            "enum": [
                                "CLASSIFY_DATA"
                            ]
                        },
                        "input": {
                            "$ref": "#/definitions/input"
                        },
                        "output": {
                            "$ref": "#/definitions/output/multi"
                        }
                    }
                },
                {
                    "properties": {
                        "type": {
                            "enum": [
                                "TRAIN_PREPROCESSOR"
                            ]
                        },
                        "input": {
                            "type": "object",
                            "$ref": "#/definitions/processorInput"
                        },
                        "output": {
                            "$ref": "#/definitions/output/single"
                        }
                    }
                },
                {
                    "properties": {
                        "type": {
                            "enum": [
                                "TRAIN_CLASSIFIER"
                            ]
                        },
                        "input": {
                            "$ref": "#/definitions/input"
                        },
                        "output": {
                            "$ref": "#/definitions/output/multi"
                        }
                    }
                },
                {
                    "properties": {
                        "type": {
                            "enum": [
                                "GENERATE_ERR"
                            ]
                        },
                        "input": {
                            "$ref": "#/definitions/input"
                        },
                        "output": {
                            "$ref": "#/definitions/output/multi"
                        }
                    }
                }
            ]
        }
    },
    "definitions": {
        "input": {
            "type": "object",
            "title": "Input Configuration",
            "properties": {
                "hasTestInput": {
                    "type": "boolean",
                    "title": "Has Test Input File",
                    "enum": [
                        true,
                        false
                    ]
                }
            },
            "dependencies": {
                "hasTestInput": {
                    "oneOf": [
                        {
                            "properties": {
                                "hasTestInput": {
                                    "enum": [
                                        true
                                    ]
                                },
                                "trainingInput": {
                                    "title": "Training Input",
                                    "$ref": "#/definitions/inputFile"
                                },
                                "testInput": {
                                    "type": "object",
                                    "properties": {
                                        "file": {
                                            "title": "Test Input",
                                            "$ref": "#/definitions/inputFile"
                                        },
                                        "applyPreprocessors": {
                                            "title": "Apply Preprocessors",
                                            "type": "boolean"
                                        }
                                    }

                                }

                            }
                        },
                        {
                            "properties": {
                                "hasTestInput": {
                                    "enum": [
                                        false
                                    ]
                                },
                                "trainingInput": {
                                    "type": "object",
                                    "title": "Training Input",
                                    "properties": {
                                        "file": {
                                            "title": "Input File",
                                            "$ref": "#/definitions/inputFile"
                                        },
                                        "subset": {
                                            "title": "Preprocessor Subset",
                                            "$ref": "#/definitions/trainingSubset"
                                        }
                                    }
                                }
                            }
                        }
                    ]
                }
            }
        },
        "output": {
            "multi": {
                "type": "object",
                "title": "Output Configuration",
                "properties": {
                    "outputFormat": {
                        "type": "string",
                        "title": "Output Format",
                        "enum": [
                            "CSV",
                            "ZJSON"
                        ]
                    }
                },
                "dependencies": {
                    "outputFormat": {
                        "oneOf": [
                            {
                                "properties": {
                                    "outputFormat": {
                                        "enum": [
                                            "CSV"
                                        ]
                                    },
                                    "defaultPath": {
                                        "type": "string",
                                        "title": "Default Path"
                                    },
                                    "identifierPaths": {
                                        "type": "array",
                                        "title": "Identifier Paths",
                                        "items": {
                                            "type": "object",
                                            "properties": {
                                                "identifier": {
                                                    "type": "string",
                                                    "title": "Identifier"
                                                },
                                                "path": {
                                                    "type": "string",
                                                    "title": "Path"
                                                }
                                            }
                                        }
                                    }
                                }
                            },
                            {
                                "properties": {
                                    "outputFormat": {
                                        "enum": [
                                            "ZJSON"
                                        ]
                                    },
                                    "filePath": {
                                        "type": "string",
                                        "title": "Output File Path"
                                    }
                                }
                            }
                        ]
                    }
                }
            },
            "single": {
                "type": "object",
                "title": "Output Configuration",
                "properties": {
                    "filePath": {
                        "type": "string",
                        "title": "Output File Path"
                    }
                }
            }
        },
        "inputFile": {
            "type": "object",
            "properties": {
                "inputFormat": {
                    "type": "string",
                    "title": "Input Format",
                    "enum": [
                        "JSONGZ",
                        "CSV",
                        "MAT",
                        "ANC"
                    ]
                }
            },
            "dependencies": {
                "inputFormat": {
                    "oneOf": [
                        {
                            "properties": {
                                "inputFormat": {
                                    "enum": [
                                        "JSONGZ"
                                    ]
                                },
                                "filePath": {
                                    "type": "string",
                                    "title": "JSON GZIP File Path"
                                }
                            }
                        },
                        {
                            "properties": {
                                "inputFormat": {
                                    "enum": [
                                        "CSV",
                                        "MAT",
                                        "ANC"
                                    ]
                                },
                                "filePath": {
                                    "type": "string",
                                    "title": "Input Index File Path"
                                },
                                "paths": {
                                    "type": "array",
                                    "title": "Sample Paths",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    ]
                }
            }
        },
        "processorInput": {
            "type": "object",
            "title": "Preprocessor Input",
            "properties": {
                "file": {
                    "title": "Input File",
                    "$ref": "#/definitions/inputFile"
                },
                "inputSet": {
                    "type": "object",
                    "title": "Input Set",
                    "properties": {
                        "single": {
                            "type": "boolean",
                            "title": "Preprocess as Single Set",
                            "enum": [
                                true,
                                false
                            ]
                        }
                    },
                    "dependencies": {
                        "single": {
                            "oneOf": [
                                {
                                    "properties": {
                                        "single": {
                                            "enum": [
                                                true
                                            ]
                                        }
                                    }
                                },
                                {
                                    "properties": {
                                        "single": {
                                            "enum": [
                                                false
                                            ]
                                        },
                                        "subset": {
                                            "title": "Preprocessor Subset",
                                            "$ref": "#/definitions/trainingSubset"
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        },
        "trainingSubset": {
            "type": "object",
            "properties": {
                "crossValidation": {
                    "type": "boolean",
                    "title": "Cross Validation",
                    "enum": [
                        true,
                        false
                    ]
                }
            },
            "dependencies": {
                "crossValidation": {
                    "oneOf": [
                        {
                            "properties": {
                                "crossValidation": {
                                    "enum": [
                                        true
                                    ]
                                },
                                "start": {
                                    "type": "integer",
                                    "title": "Cross Validation Start"
                                },
                                "length": {
                                    "type": "integer",
                                    "title": "Cross Validation Length"
                                }
                            }
                        },
                        {
                            "properties": {
                                "crossValidation": {
                                    "enum": [
                                        false
                                    ]
                                },
                                "trainingSize": {
                                    "type": "integer",
                                    "title":  "Training Size"
                                }
                            }
                        }
                    ]
                }
            }
        },
        "preprocessor": {
            "title": "Preprocessor",
            "type": "object",
            "properties": {
                "category": {
                    "type": "string",
                    "enum": [
                        "TRANSFORMATION",
                        "DIMENSIONALITY_REDUCTION"
                    ]
                }
            },
            "dependencies": {
                "category": {
                    "oneOf": [
                        {
                            "properties": {
                                "category": {
                                    "enum": [
                                        "TRANSFORMATION"
                                    ]
                                },
                                "method": {
                                    "type": "string",
                                    "enum": [
                                        "OPTIMAL_WP",
                                        "DERIVATIVE",
                                        "MAGNITUDE_SPECTRUM",
                                        "PSD",
                                        "L1_NORM",
                                        "L2_NORM",
                                        "LINF_NORM",
                                        "LTN",
                                        "SNV",
                                        "LLSR",
                                        "LLSRDTW"
                                    ]
                                }
                            },
                            "dependencies": {
                                "method": {
                                    "oneOf": [
                                        {
                                            "properties": {
                                                "method": {
                                                    "enum": [
                                                        "OPTIMAL_WP"
                                                    ]
                                                },
                                                "parameters": {
                                                    "$ref": "#/definitions/preprocessorParams/transform/optimalWavelet"
                                                }
                                            }
                                        },
                                        {
                                            "properties": {
                                                "method": {
                                                    "enum": [
                                                        "DERIVATIVE"
                                                    ]
                                                },
                                                "parameters": {
                                                    "$ref": "#/definitions/preprocessorParams/transform/columnOnlyParam"
                                                }
                                            }
                                        },
                                        {
                                            "properties": {
                                                "method": {
                                                    "enum": [
                                                        "MAGNITUDE_SPECTRUM"
                                                    ]
                                                }
                                            }
                                        },
                                        {
                                            "properties": {
                                                "method": {
                                                    "enum": [
                                                        "PSD"
                                                    ]
                                                }
                                            }
                                        },
                                        {
                                            "properties": {
                                                "method": {
                                                    "enum": [
                                                        "L1_NORM"
                                                    ]
                                                },
                                                "parameters": {
                                                    "$ref": "#/definitions/preprocessorParams/transform/columnOnlyParam"
                                                }
                                            }
                                        },
                                        {
                                            "properties": {
                                                "method": {
                                                    "enum": [
                                                        "L2_NORM"
                                                    ]
                                                },
                                                "parameters": {
                                                    "$ref": "#/definitions/preprocessorParams/transform/columnOnlyParam"
                                                }
                                            }
                                        },
                                        {
                                            "properties": {
                                                "method": {
                                                    "enum": [
                                                        "LINF_NORM"
                                                    ]
                                                },
                                                "parameters": {
                                                    "$ref": "#/definitions/preprocessorParams/transform/columnOnlyParam"
                                                }
                                            }
                                        },
                                        {
                                            "properties": {
                                                "method": {
                                                    "enum": [
                                                        "LTN"
                                                    ]
                                                },
                                                "parameters": {
                                                    "$ref": "#/definitions/preprocessorParams/transform/ltn"
                                                }
                                            }
                                        },
                                        {
                                            "properties": {
                                                "method": {
                                                    "enum": [
                                                        "SNV"
                                                    ]
                                                },
                                                "parameters": {
                                                    "$ref": "#/definitions/preprocessorParams/transform/columnOnlyParam"
                                                }
                                            }
                                        },
                                        {
                                            "properties": {
                                                "method": {
                                                    "enum": [
                                                        "LLSR"
                                                    ]
                                                },
                                                "parameters": {
                                                    "$ref": "#/definitions/preprocessorParams/transform/columnOnlyParam"
                                                }
                                            }
                                        },
                                        {
                                            "properties": {
                                                "method": {
                                                    "enum": [
                                                        "LLSRDTW"
                                                    ]
                                                },
                                                "parameters": {
                                                    "$ref": "#/definitions/preprocessorParams/transform/llsrdtw"
                                                }
                                            }
                                        }
                                    ]
                                }
                            }
                        },
                        {
                            "properties": {
                                "category": {
                                    "enum": [
                                        "DIMENSIONALITY_REDUCTION"
                                    ]
                                },
                                "method": {
                                    "type": "string",
                                    "enum": [
                                        "PCA",
                                        "FUZZY_FEATURE",
                                        "GEOMETRIC",
                                        "FEATURE_INDEX"
                                    ]
                                }
                            },
                            "dependencies": {
                                "method": {
                                    "oneOf": [
                                        {
                                            "properties": {
                                                "method": {
                                                    "enum": [
                                                        "PCA"
                                                    ]
                                                },
                                                "parameters": {
                                                    "$ref": "#/definitions/preprocessorParams/dimensionalityReduction/pca"
                                                }
                                            }
                                        },
                                        {
                                            "properties": {
                                                "method": {
                                                    "enum": [
                                                        "FUZZY_FEATURE"
                                                    ]
                                                },
                                                "parameters": {
                                                    "$ref": "#/definitions/preprocessorParams/dimensionalityReduction/fuzzyFeatures"
                                                }
                                            }
                                        },
                                        {
                                            "properties": {
                                                "method": {
                                                    "enum": [
                                                        "GEOMETRIC"
                                                    ]
                                                },
                                                "parameters": {
                                                    "$ref": "#/definitions/preprocessorParams/dimensionalityReduction/geometric"
                                                }
                                            }
                                        },
                                        {
                                            "properties": {
                                                "method": {
                                                    "enum": [
                                                        "FEATURE_INDEX"
                                                    ]
                                                },
                                                "parameters": {
                                                    "$ref": "#/definitions/preprocessorParams/dimensionalityReduction/featureIndex"
                                                }
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                }
            }
        },
        "preprocessorParams": {
            "transform": {
                "optimalWavelet": {
                    "type": "object",
                    "properties": {
                        "wavelet": {
                            "type": "string",
                            "enum": [
                                "COIF06",
                                "DAUB02",
                                "DAUB04",
                                "LEGE04",
                                "LEGE06"
                            ]
                        },
                        "toLevel": {
                            "type": "integer"
                        },
                        "r": {
                            "type": "number"
                        },
                        "columns": {
                            "$ref": "#/definitions/preprocessorParams/transform/columns"
                        }
                    }
                },
                "ltn": {
                    "type": "object",
                    "properties": {
                        "standardTime": {
                            "type": "number"
                        },
                        "columns": {
                            "$ref": "#/definitions/preprocessorParams/transform/columns"
                        }
                    }
                },
                "llsrdtw": {
                    "type": "object",
                    "properties": {
                        "bandwidth": {
                            "type": "integer"
                        },
                        "columns": {
                            "$ref": "#/definitions/preprocessorParams/transform/columns"
                        }
                    }
                },
                "columnOnlyParam": {
                    "type": "object",
                    "properties": {
                        "columns": {
                            "$ref": "#/definitions/preprocessorParams/transform/columns"
                        }
                    }
                },
                "columns": {
                    "type": "object",
                    "properties": {
                        "transformAll": {
                            "type": "boolean",
                            "enum": [
                                true,
                                false
                            ]
                        }
                    },
                    "dependencies": {
                        "transformAll": {
                            "oneOf": [
                                {
                                    "properties": {
                                        "transformAll": {
                                            "enum": [
                                                true
                                            ]
                                        }
                                    }
                                },
                                {
                                    "properties": {
                                        "transformAll": {
                                            "enum": [
                                                false
                                            ]
                                        },
                                        "useColumns": {
                                            "type": "array",
                                            "items": {
                                                "type": "integer"
                                            }
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            },
            "dimensionalityReduction": {
                "pca": {
                    "type": "object",
                    "properties": {
                        "analysis": {
                            "type": "string",
                            "enum": [
                                "COVARIANCE",
                                "CORRELATION"
                            ]
                        },
                        "components": {
                            "type": "integer"
                        }
                    }
                },
                "fuzzyFeatures": {
                    "type": "object",
                    "properties": {
                        "features": {
                            "type": "integer"
                        }
                    }
                },
                "featureIndex": {
                    "type": "object",
                    "properties": {
                        "indexes": {
                            "type": "array",
                            "items": {
                                "type": "integer"
                            }
                        }
                    }
                },
                "geometric": {
                    "type": "object",
                    "properties": {
                        "templates": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "templateType": {
                                        "type": "string",
                                        "enum": [
                                            "AREA",
                                            "EUCLIDEAN",
                                            "MEAN",
                                            "STANDARD",
                                            "EXTREMA"
                                        ]
                                    },
                                    "columns": {
                                        "type": "array",
                                        "items": {
                                            "type": "integer"
                                        }
                                    }
                                },
                                "dependencies": {
                                    "templateType": {
                                        "oneOf": [
                                            {
                                                "properties": {
                                                    "templateType": {
                                                        "enum": [
                                                            "AREA",
                                                            "EUCLIDEAN",
                                                            "MEAN",
                                                            "STANDARD"
                                                        ]
                                                    }
                                                }
                                            },
                                            {
                                                "properties": {
                                                    "templateType": {
                                                        "enum": [
                                                            "EXTREMA"
                                                        ]
                                                    },
                                                    "start": {
                                                        "type": "string",
                                                        "enum": [
                                                            "LOCAL_MAX",
                                                            "LOCAL_MIN"
                                                        ]
                                                    },
                                                    "points": {
                                                        "type": "array",
                                                        "items": {
                                                            "type": "object",
                                                            "properties": {
                                                                "sensitivity": {
                                                                    "type": "integer"
                                                                },
                                                                "dataModel": {
                                                                    "type": "string",
                                                                    "enum": [
                                                                        "TIME",
                                                                        "AMPLITUDE",
                                                                        "TIME_AMPLITUDE"
                                                                    ]
                                                                },
                                                                "startAmplitude": {
                                                                    "type": "integer"
                                                                },
                                                                "searchIndexLimit": {
                                                                    "type": "integer"
                                                                },
                                                                "hasRecovery": {
                                                                    "type": "boolean",
                                                                    "enum": [
                                                                        true,
                                                                        false
                                                                    ]
                                                                }
                                                            },
                                                            "dependencies": {
                                                                "hasRecovery": {
                                                                    "oneOf": [
                                                                        {
                                                                            "properties": {
                                                                                "hasRecovery": {
                                                                                    "enum": [
                                                                                        true
                                                                                    ]
                                                                                },
                                                                                "recovery": {
                                                                                    "type": "object",
                                                                                    "properties": {
                                                                                        "firstTriangularLength": {
                                                                                            "type": "integer"
                                                                                        },
                                                                                        "firstPointSensitivity": {
                                                                                            "type": "integer"
                                                                                        },
                                                                                        "secondTriangularLength": {
                                                                                            "type": "integer"
                                                                                        },
                                                                                        "secondPointSensitivity": {
                                                                                            "type": "integer"
                                                                                        }
                                                                                    }
                                                                                }
                                                                            }
                                                                        },
                                                                        {
                                                                            "properties": {
                                                                                "hasRecovery": {
                                                                                    "enum": [
                                                                                        false
                                                                                    ]
                                                                                }
                                                                            }
                                                                        }
                                                                    ]
                                                                }
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        ]
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
